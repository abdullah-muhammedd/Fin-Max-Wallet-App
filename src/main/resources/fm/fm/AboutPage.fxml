                    <ImageView>
                        <Image url="@linkedin.png"/>
                    </ImageView>
                </graphic>
            </Hyperlink>
        </graphic>
    </Button>
    <Button layoutX="373.0" layoutY="138.0" mnemonicParsing="false" style="-fx-background-color: transparent;">
        <graphic>
            <Hyperlink fx:id="github_link" contentDisplay="GRAPHIC_ONLY" onAction="#githubLinkOnAction"
                       text="https://github.com/abdullah-muhammedd">
                <graphic>
                    <ImageView>
                        <Image url="@github.png"/>
                    </ImageView>
                </graphic>
            </Hyperlink>
        </graphic>
    </Button>
    <Label layoutX="261.0" layoutY="67.0" text="Email:">
        <font>
            <Font name="Arial" size="15.0"/>
