    <Label layoutX="456.0" layoutY="39.0" text="Abdullah Muhammed">
        <font>
            <Font name="Arial" size="15.0"/>
        </font>
    </Label>
    <Button layoutX="261.0" layoutY="138.0" mnemonicParsing="false" prefHeight="46.0" prefWidth="42.0"
            style="-fx-background-color: transparent;">
        <graphic>
            <Hyperlink fx:id="facebook_link" alignment="CENTER" contentDisplay="GRAPHIC_ONLY"
                       onAction="#facebookLinkOnAction" prefHeight="23.0" prefWidth="0.0"
                       text="https://www.facebook.com/profile.php?id=100083643485686">
                <graphic>
                    <ImageView>
                        <Image url="@facebook.png"/>
                    </ImageView>
                </graphic>
            </Hyperlink>
        </graphic>
    </Button>
    <Button layoutX="317.0" layoutY="138.0" mnemonicParsing="false" style="-fx-background-color: transparent;">
        <graphic>
            <Hyperlink fx:id="linkedin_link" contentDisplay="GRAPHIC_ONLY" onAction="#linkedinLinkOnAction"
                       text="https://www.linkedin.com/in/abdullah-muhammed-95b143216/">
