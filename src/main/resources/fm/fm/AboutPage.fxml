    </Label>
    <Label layoutX="310.0" layoutY="67.0" text="dev.abdullah.muhammed@gmail.com">
        <font>
            <Font name="Arial" size="15.0"/>
        </font>
    </Label>
    <Label layoutX="268.0" layoutY="101.0" prefHeight="17.0" prefWidth="70.0" text="Contacts:">
        <font>
            <Font name="Arial" size="15.0"/>
        </font>
    </Label>
    <Separator layoutX="402.0" layoutY="193.0" prefHeight="2.0" prefWidth="393.0"/>
</AnchorPane>
